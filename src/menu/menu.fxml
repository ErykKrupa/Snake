<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
      prefHeight="420.0" prefWidth="540.0" fx:controller="menu.MenuController">
    <Label alignment="CENTER" layoutX="201.0" layoutY="14.0"
           prefHeight="51.0" prefWidth="139.0" text="Welcome in" textAlignment="CENTER">
        <font>
            <Font name="Monotype Corsiva" size="18.0" />
        </font>
    </Label>
    <Label alignment="CENTER" layoutX="89.0" layoutY="40.0" prefHeight="62.0" prefWidth="364.0" text="Snake Game">
        <font>
             <Font name="Monotype Corsiva" size="50.0" />
        </font>
    </Label>
    <Button layoutX="196.0" layoutY="141.0" onAction="#playClick" prefHeight="50.0" prefWidth="150.0" text="Play">
        <font>
            <Font name="Monotype Corsiva" size="25.0" />
         </font>
    </Button>
    <Button layoutX="195.0" layoutY="210.0" onAction="#highScoresClick"
            prefHeight="50.0" prefWidth="150.0" text="High Scores">
        <font>
            <Font name="Monotype Corsiva" size="25.0" />
        </font>
    </Button>
    <Button layoutX="196.0" layoutY="281.0" onAction="#creditClick" prefHeight="50.0" prefWidth="150.0" text="Credit">
        <font>
            <Font name="Monotype Corsiva" size="25.0" />
        </font>
    </Button>
    <Button layoutX="196.0" layoutY="347.0" onAction="#exitClick" prefHeight="50.0" prefWidth="150.0" text="Exit">
        <font>
            <Font name="Monotype Corsiva" size="25.0" />
        </font>
    </Button>

</Pane>
